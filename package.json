{
  "name": "next-openapi-gen",
  "version": "0.0.2",
  "description": "Automatically generate OpenAPI documentation for Next.js API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "bin": {
    "next-openapi-gen": "./dist/index"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tazo90/next-openapi-gen.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "openapi",
    "swagger",
    "docs",
    "api"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Mariusz Winnik",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.25.7",
    "@babel/traverse": "^7.25.7",
    "@babel/types": "^7.25.7",
    "commander": "^12.1.0",
    "fs-extra": "^10.0.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "typescript": "^4.5.4"
  }
}
